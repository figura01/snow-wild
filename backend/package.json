{
  "name": "demo-graphql",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node-dev --exit-child --watch src --poll src/index.ts",
    "test": "jest --watch",
    "testCI": "jest",
    "build": "npx tsc",
    "startjs": "cd /app/dist && node index.js",
    "fix": "npm run format && npm run lint:fix",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "resetDb": "ts-node-dev src/resetDb.ts"
  },
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@graphql-tools/schema": "^10.0.2",
    "argon2": "^0.31.2",
    "axios": "^1.7.2",
    "class-validator": "^0.14.0",
    "cookies": "^0.9.1",
    "cors": "^2.8.5",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.23.0",
    "jose": "^5.2.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.7",
    "stripe": "^16.4.0",
    "type-graphql": "^2.0.0-beta.3",
    "typeorm": "^0.3.17",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@graphql-tools/mock": "^9.0.1",
    "@types/cookies": "^0.9.0",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.19.1",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.9.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
