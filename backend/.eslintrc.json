{
    "root": true,
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "env": {
        "node": true,
        "es2021": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": ["react", "@typescript-eslint"],
    "rules": {
        "@typescript-eslint/no-unused-vars" : "warn",
        "no-undef":"warn"
    }
}
